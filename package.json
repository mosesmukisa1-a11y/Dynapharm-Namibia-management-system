{
  "name": "dynapharm-cloud",
  "version": "2.0.0",
  "description": "Dynapharm International Namibia - Cloud Health Management System",
  "main": "dynapharm_backend.py",
  "type": "module",
  "scripts": {
    "start": "python3 dynapharm_backend.py",
    "dev": "python3 dynapharm_backend.py",
    "test": "curl http://localhost:8001/api/health",
    "auto-commit": "node tools/auto-commit.js",
    "guard": "node tools/guard-inline-events.js",
    "seed:users": "node scripts/seed-users.js",
    "seed:users:file": "node scripts/seed-users.js --file",
    "migrate:user-passwords": "node scripts/migrate-user-passwords.js",
    "import:local-users": "node scripts/import-local-users.js",
    "predeploy": "npm run guard",
    "lint": "eslint assets/js/main.js assets/js/sw-register.js index.html",
    "format": "prettier --write index.html assets/js/**/*.js assets/css/**/*.css README.md",
    "format:check": "prettier --check index.html assets/js/**/*.js assets/css/**/*.css README.md"
  },
  "keywords": [
    "healthcare",
    "management",
    "namibia",
    "dynapharm",
    "medical"
  ],
  "author": "Moses Mukisa",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mosesmukisa1-a11y/dynapharm-cloud.git"
  },
  "bugs": {
    "url": "https://github.com/mosesmukisa1-a11y/dynapharm-cloud/issues"
  },
  "homepage": "https://dynapharm-cloud.vercel.app",
  "dependencies": {
    "pg": "^8.16.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie": "^0.6.0"
  },
  "devDependencies": {
    "chokidar": "^3.6.0",
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3"
  }
}
